//https://www.hackerrank.com/contests/ca1k23sheven/challenges/prime-anagram-pairs

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

class Main{
    public:
    vector<int> isprime(int n){
    vector<bool> prime(n, true);
    for (int p = 2; p * p <= n; p++) {
        if (prime[p] == true) {
            for (int i = p * p; i <= n; i += p)
                prime[i] = false;
        }
    }
    
    vector<int> res;
    for (int p = 13; p <= n; p++){
        if (prime[p]){ 
            res.push_back(p);
        }
    }
    return res;
    }
};
int main() {
    int num;
    cin>>num;
    Main obj;
    vector<int> res=obj.isprime(num);
    vector<int> revs;
    for(int i=0;i<res.size();i++){
        int t=res[i];
        if(find(revs.begin(),revs.end(),t)!=revs.end()) continue;
        int rev=0;
        while(t>0){
                rev=rev*10+t%10;
                t=t/10;
        }
        if(rev!=0){
            revs.push_back(rev);
           if(find(res.begin(),res.end(),rev)!=res.end()){
            cout<<"("<<res[i]<<" , "<<rev<<")"<<endl;
           } 
        }
    }
    return 0;
}
