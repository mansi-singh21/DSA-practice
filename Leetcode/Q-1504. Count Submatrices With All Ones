class Solution {
public:
    int d1count(vector<int>&vec){
        int c=0;
        int subc=0;
        for(int i=0;i<vec.size();i++){
            if(vec[i]==0){
                c=0;
            }else{
                c++;
            }
            subc+=c;
        }
        return subc;
    }
    int numSubmat(vector<vector<int>>& mat) {
        int m=mat.size();
        int n=mat[0].size();
        int result=0;
        for(int startrow=0;startrow<m;startrow++){
            vector<int>vec(n,1);
            for(int endrow=startrow;endrow<m;endrow++){

                for(int i=0;i<n;i++){
                    vec[i]=vec[i]&mat[endrow][i];
                }
                result+=d1count(vec);
            }
        }
        return result;
    }
};
