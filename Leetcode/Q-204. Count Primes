class Solution {
public:
    int countPrimes(int n) {
        if (n == 0 || n == 1) return 0;
        vector<int> res;
        vector<bool> prime(n,true);
        for(int p=2;p*p<=n;p++){
            if(prime[p]){
                for(int i=p*p;i<n;i+=p){
                    prime[i]=false;
                }
            }
        }
        for(int i=2;i<n;i++){
            if(prime[i]){
                res.push_back(i);
            }
        }
        return res.size();
    }
};
